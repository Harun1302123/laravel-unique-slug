{
    "name": "harunurrashid/laravel-unique-slug",
    "description": "A unique slug for laravel",
    "keywords": [
        ":unique_slug",
        "laravel",
        ":package_slug"
    ],
    "homepage": "https://github.com/harunBdtask/laravel-unique-slug",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HarunurRashid\\LaravelUniqueSlug\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Harunur Rashid",
            "email": "ahmharunurrashid@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HarunurRashid\\LaravelUniqueSlug\\UniqueSlugServiceProvider"
            ],
            "aliases": {
                "UniqueSlug": "HarunurRashid\\LaravelUniqueSlug\\UniqueSlug"
            }
        }
    }
}
